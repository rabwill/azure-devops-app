{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "resourceBaseName": {
      "value": "ME${{RESOURCE_SUFFIX}}"
    },
    "botAadAppClientId": {
      "value": "${{BOT_ID}}"
    },
    "botAadAppClientSecret": {
      "value": "${{SECRET_BOT_PASSWORD}}"
    },
    "webAppSKU": {
      "value": "B1"
    },
    "botDisplayName": {
      "value": "DevOps Workflow Accelerator"
    },
    "azpat": {
      "value": "${{AZURE_PERSONAL_ACCESS_TOKEN}}"
    },
    "previewimage": {
      "value": "${{PREVIEW_IMAGE}}"
    },
    "wiUrl": {
      "value": "${{WORK_ITEM_URL}}"
    },
    "orgName": {
      "value": "${{ORG_NAME}}"
    },
    "projectName": {
      "value": "${{PROJECT_NAME}}"
    },
    "host": {
      "value": "${{BOT_DOMAIN}}"
    }

  }
}